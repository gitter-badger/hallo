<oi-price>
  <div class="card-price">
    <div class="price-widget">
      <span class="price-prefix" if={ prefix }>{ prefix }</span>
      <span class="price-value"  if={ price  }>{ price  }</span>
      <span class="price-cents"  if={ cents  }>{ cents  }</span>
      <span class="price-sufix"  if={ sufix  }>{ sufix  }</span>
    </div>
  </div>
</oi-price>

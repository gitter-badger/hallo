.content
  .content_header_container
    include _content-header.jade
    
  .content_table_container
    .content_table_header
      ul
        each group in channelsgroup.tv
          li
            h4.oi-channels-lists_list-tv_title=group.label
              span= ' [' + group.channels.length + ']'

        li: h4.content_table_header_add-on
          span Turine Sua TV HD
          span Opcionais para a sua Oi TV HD

    .content_table
      include _channel.jade

{
  "firebase": "oioi",
  "public": "dist",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "headers":[
    {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    },
    {
      "source" : "**/*.@(woff|woff2|eot|svg|ttf)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    },
    {
      "source" : "**/*.@(css)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    },
    {
      "source" : "**/*.@(js|json)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    },
    {
      "source" : "**/*.@(html|htm)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    }
  ]
}
